{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./../dist/app",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es6",
    "lib": ["es6", "es7", "es2017.object", "dom"],
    "jsx": "react",
    "sourceMap": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules",
    "./internal/timeline_ts_exclude/**/*.ts",
    "./internal/drawing_ts_exclude/**/*.ts",
    "./internal/renderer/drawing_ts_exclude/**/*.ts",
    "./internal/renderer/drawing/**/*.ts",
    "./window/view/effect-control_ts_exclude/**/*.ts",
    "./window/view/effect-control__ts_exclude/**/*.ts",
    "./window/view/timeline/controller_ts_exclude/**/*.ts",
    "./window/view/timeline/right_ts_exclude/**/*.ts"
  ]/*,
  "files": [
    "typings.d.ts",
    "window/index.tsx",
    "window/initializer.ts",
    "internal/index.ts",
    "internal/renderer/index.ts"
  ]*/
}